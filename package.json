{
  "name": "linear-file-sync",
  "version": "1.0.0",
  "description": "A TypeScript application that synchronizes Linear.app issues with a text file",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:fetch": "ts-node src/testFetch.ts"
  },
  "keywords": [
    "linear",
    "sync",
    "issues"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@linear/sdk": "^1.0.0",
    "chokidar": "^3.5.3",
    "diff": "^5.1.0"
  },
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
